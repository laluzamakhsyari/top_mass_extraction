            "nbins"}], "]"}], "[", 
          RowBox[{"[", "m", "]"}], "]"}]}], " ", ";", "\[IndentingNewLine]", 
        RowBox[{"zeropos", "=", 
         RowBox[{"Position", "[", 
          RowBox[{"perrorlist", ",", "0."}], "]"}]}], ";", " ", 
        "\[IndentingNewLine]", 
        RowBox[{"perrorlist", "=", 
         RowBox[{"Delete", "[", 
          RowBox[{"perrorlist", ",", "zeropos"}], "]"}]}], ";", " ", 
        RowBox[{"(*", 
         RowBox[{"delete", " ", "zero", " ", "error", " ", "values"}], "*)"}],
         "\[IndentingNewLine]", 
        RowBox[{"pvaluelist", "=", 
         RowBox[{"Delete", "[", 
          RowBox[{"pvaluelist", ",", "zeropos"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"chisq", "=", 
         RowBox[{"ParallelTable", "[", 
          RowBox[{
           RowBox[{"chitilde", "[", 
            RowBox[{"pvaluelist", ",", "perrorlist", ",", 
