Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"fittingmass", "[", 
    RowBox[{
    "nevents_", ",", "binw_", ",", "binsreal_", ",", "bins_", ",", "nbins_", 
     ",", "m_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"Reap", "[", "\[IndentingNewLine]", 
     RowBox[{"Block", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "pvaluelist", ",", "perrorlist", ",", "zeropos", ",", "datachi", ",", 
